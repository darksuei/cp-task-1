{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "moduleFileExtensions": ["js", "jsx", "ts", "tsx"],
  "transform": {
    "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"]
}
